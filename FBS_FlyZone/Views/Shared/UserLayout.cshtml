@{
    Layout = null;
}

@{
	ViewData["Title"] = "Home Page";
}


<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" lang="zxx">

<!-- Mirrored from themeim.com/demo/flynext/index-dark.html by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 28 Mar 2025 17:43:04 GMT -->
<head>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>@ViewData["Title"] - FlyZone</title>
    <link rel="preconnect" href="https://fonts.googleapis.com/">
    <link rel="preconnect" href="https://fonts.gstatic.com/" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Mulish:ital,wght@0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&amp;family=Playfair+Display:ital,wght@0,400;0,500;0,600;0,700;0,800;0,900;1,400;1,500;1,600;1,700;1,800;1,900&amp;display=swap"
          rel="stylesheet">
    <!-- fontawesome css link -->
    <link rel="stylesheet" href="~/flynext/assets/css/fontawesome-all.min.css">
    <!-- bootstrap css link -->
    <link rel="stylesheet" href="~/flynext/assets/css/bootstrap.min.css">
	<!-- bootstrap css link -->
    <!-- favicon -->
    <link rel="shortcut icon" href="~/flynext/assets/images/fav.png" type="image/x-icon">
    <!-- swipper css link -->
    <link rel="stylesheet" href="~/flynext/assets/css/swiper.min.css">
    <!-- lightcase css links -->
    <link rel="stylesheet" href="~/flynext/assets/css/lightcase.css">
    <!-- odometer css link -->
    <link rel="stylesheet" href="~/flynext/assets/css/odometer.css">
    <!-- line-awesome-icon css -->
    <link rel="stylesheet" href="~/flynext/assets/css/icomoon.css">
    <!-- line-awesome-icon css -->
    <link rel="stylesheet" href="~/flynext/assets/css/line-awesome.min.css">
    <!-- animate.css -->
    <link rel="stylesheet" href="~/flynext/assets/css/animate.css">
    <!-- aos.css -->
    <link rel="stylesheet" href="~/flynext/assets/css/aos.css">
    <!-- nice select css -->
    <link rel="stylesheet" href="~/flynext/assets/css/nice-select.css">
    <!-- main style css link -->
    <link rel="stylesheet" href="~/flynext/assets/css/style.css">

</head>
<body>

	@await Html.PartialAsync("PartialHeader")

	@RenderBody()

	@await Html.PartialAsync("PartialFooter")

    <!-- jquery -->
    <script src="~/flynext/assets/js/jquery.js"></script>

    <!-- bootstrap js -->
    <script src="~/flynext/assets/js/bootstrap.min.js"></script>
    <!-- swipper js -->
    <script src="~/flynext/assets/js/swiper.min.js"></script>
    <!-- lightcase js-->
    <script src="~/flynext/assets/js/lightcase.js"></script>
    <!-- odometer js -->
    <script src="~/flynext/assets/js/odometer.min.js"></script>
    <!-- viewport js -->
    <script src="~/flynext/assets/js/viewport.jquery.js"></script>
    <!-- progress js -->
    <script src="~/flynext/assets/js/circle-progress.min.js"></script>
    <!-- aos js file -->
    <script src="~/flynext/assets/js/aos.js"></script>
    <!-- nice select js -->
    <script src="~/flynext/assets/js/jquery.nice-select.js"></script>
    <!-- main -->
    <script src="~/flynext/assets/js/main.js"></script>
</body>
</html>